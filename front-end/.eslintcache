 } from \"react\";\nimport { listReservations, listTables } from \"../utils/api\";\nimport { next, previous, today } from \"../utils/date-time\";\nimport useQuery from \"../utils/useQuery\";\nimport { useHistory, useRouteMatch } from \"react-router-dom\";\nimport ErrorAlert from \"../layout/errors/ErrorAlert\";\nimport ViewAllReservation from \"./ViewReservations\";\n\n\n/**\n * Defines the dashboard page.\n * @param date\n *  the date for which the user wants to view reservations.\n */\n\nfunction Dashboard({ date, setDate }) {\n\n  // useStates\n  const [reservations, setReservations] = useState([]);\n  const [reservationsError, setReservationsError] = useState(null);\n  const [tables, setTables] = useState([]);\n  const [tablesError, setTablesError] = useState(null);\n\n  const history = useHistory();\n  const query = useQuery();\n  const route = useRouteMatch();\n\n  // to get the date to change\n  useEffect(() => {\n    function updateDate() {\n      const queryDate = query.get(\"date\");\n      if (queryDate) {\n        setDate(queryDate);\n      } else {\n        setDate(today());\n      }\n    }\n    updateDate();\n  }, [query, route, setDate]);\n  useEffect(loadDashboard, [date]);\n  \n\n\n  // calls the listReservations from utils.api\n  function loadDashboard() {\n    const abortController = new AbortController();\n    setReservationsError(null);\n    setTablesError(null);\n    listReservations({ date }, abortController.signal)\n      .then(setReservations)\n      .catch(setReservationsError);\n    listTables(abortController.signal)\n      .then(setTables)\n      .catch(setTablesError);\n    return () => abortController.abort();\n  };\n\n  // reservations List - print all the reservations \n  const reservationList = reservations.map((reservation) => {\n    if (reservation.status === \"finished\" || reservation.status === \"canceled\") return null;\n    <ViewAllReservation key={reservation.reservation_id} reservation={reservation} />   \n  });\n\n  // tables List \n  const tablesList = tables.map((table) => {\n    // have a table View here > key={table.table_id} table={table}\n  });\n\n  return (\n    <main>\n      <h1>Dashboard</h1>\n      <div className=\"d-md-flex mb-3\">\n        <button className=\"btn btn-info\" onClick={() => history.push(`/dashboard?date=${previous(date)}`)}> Previous </button>\n        <button className=\"btn btn-info\" onClick={() => history.push(`/dashboard?date=${today(date)}`)}> Today </button>\n        <button className=\"btn btn-info\" onClick={() => history.push(`/dashboard?date=${next(date)}`)}> Next </button>\n      </div>\n      <div>\n        <h2>Reservations for {date} </h2>\n      </div>\n\n      <div>\n        <div className=\"container fluid\">{reservationList}</div>\n      </div>\n\n      <div>\n        <h3>Tables</h3>\n        <div>{tablesList}</div>\n      </div>\n\n      <ErrorAlert error={reservationsError} />\n      <ErrorAlert error={tablesError} />\n    </main>\n  );\n}\n\nexport default Dashboard;\n","/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/utils/date-time.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/utils/api.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/utils/format-reservation-date.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/layout/errors/NotFound.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/layout/components/reservation/NewReservation.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/dashboard/ViewReservations.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/layout/components/reservation/newResForm.js",["74"],"import React from \"react\";\nimport { useHistory } from \"react-router-dom\";\n\nfunction newResForm({ handleChange, handleSubmit }) {\n    const history = useHistory(); \n\n    return (\n        <div