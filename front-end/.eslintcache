-1\"\n              id=\"reservation_time\"\n              type=\"time\"\n              name=\"reservation_time\"\n              onChange={handleChange}\n              //value={form[reservation_time]}\n              pattern=\"[0-9]{2}:[0-9]{2}\"\n              placeholder=\"HH:MM\"\n            />\n    \n             {/* People in Reservation */}\n             <label htmlFor=\"people\"> People In Party: </label>\n            <input\n              className=\"form-control my-1\"\n              id=\"people\"\n              type=\"number\"\n              name=\"people\"\n              onChange={handleChange}\n              //value={form[people]}\n              min={1}\n              placeholder=\"2\"\n            />\n    \n            {/* Submit Button */}\n            <button className=\"btn btn-primary\"> Submit </button>\n    \n            {/* Cancel Button goes back previous page */}\n            <button className=\"btn btn-secondary\" onClick={() => history.goBack()}> Cancel </button>\n          </form>\n        </div>\n      );\n}\n\nexport default newResForm;",["75","76"],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/layout/components/reservation/NewResForm.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/layout/errors/ErrorAlert.js",[],"/Users/keriwilkins/Documents/git_Repos/restaurant-reservation/front-end/src/utils/useQuery.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":59,"column":58,"nodeType":"83","messageId":"84","endLine":59,"endColumn":60},{"ruleId":"81","severity":1,"message":"85","line":65,"column":41,"nodeType":"83","messageId":"86","endLine":65,"endColumn":43},{"ruleId":"87","severity":2,"message":"88","line":5,"column":21,"nodeType":"89","endLine":5,"endColumn":31},{"ruleId":"77","replacedBy":"90"},{"ruleId":"79","replacedBy":"91"},"no-native-reassign",["92"],"no-negated-in-lhs",["93"],"array-callback-retur